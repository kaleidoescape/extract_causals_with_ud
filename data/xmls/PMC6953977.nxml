				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \begin{aligned} \displaystyle&#x00026;{\mathcal {L}}({\mathbb {V},\mathbb {Y},\mu }) \, = \left||{\mathbf {N}} \right||^{p}_{S_p}+\lambda \left||{\mathbf {W}} \circ {\mathbf {E}} \right||^{q}_{q} \\&#x00026;+\left\langle {\mathbf {M} - \mathbf {L}-\mathbf {E},{\varvec{\Lambda }_1}}\right\rangle +\left\langle {\mathbf {N} - \mathcal {H}(\mathbf {L}),{\varvec{\Lambda }_2}}\right\rangle \\&#x00026;+ \frac{\mu }{2}\Big ( \left|| \mathbf {M} - \mathbf {L}-\mathbf {E} \right||^2_F +\left|| \mathbf {N} - \mathcal {H}(\mathbf {L}) \right||^2_F \Big ), \end{aligned} \end{aligned}$$\end{document}</tex-math><mml:math id="M356" display="block"><mml:mrow><mml:mtable columnspacing="0.5ex"><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mtable columnspacing="0.5ex"><mml:mtr><mml:mtd columnalign="right"><mml:mstyle displaystyle="true" scriptlevel="0"><mml:mrow/></mml:mstyle></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mi mathvariant="script">L</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mrow><mml:mi mathvariant="double-struck">V</mml:mi><mml:mo>,</mml:mo><mml:mi mathvariant="double-struck">Y</mml:mi><mml:mo>,</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:mrow><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mspace width="0.166667em"/><mml:mo>=</mml:mo><mml:mfenced close="|" open="|" separators=""><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="bold">N</mml:mi></mml:mfenced><mml:msubsup><mml:mrow><mml:msubsup><mml:mo stretchy="false">|</mml:mo><mml:msub><mml:mi>S</mml:mi><mml:mi>p</mml:mi></mml:msub><mml:mi>p</mml:mi></mml:msubsup><mml:mo>+</mml:mo><mml:mi mathvariant="italic">&#x003bb;</mml:mi><mml:mfenced close="|" open="|" separators=""><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="bold">W</mml:mi><mml:mo>&#x02218;</mml:mo><mml:mi mathvariant="bold">E</mml:mi></mml:mfenced><mml:mo stretchy="false">|</mml:mo></mml:mrow><mml:mi>q</mml:mi><mml:mi>q</mml:mi></mml:msubsup></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow/></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>+</mml:mo><mml:mfenced close="&#x0232a;" open="&#x02329;" separators=""><mml:mrow><mml:mi mathvariant="bold">M</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="bold">L</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="bold">E</mml:mi><mml:mo>,</mml:mo><mml:msub><mml:mrow><mml:mi mathvariant="bold">&#x0039b;</mml:mi></mml:mrow><mml:mn>1</mml:mn></mml:msub></mml:mrow></mml:mfenced><mml:mo>+</mml:mo><mml:mfenced close="&#x0232a;" open="&#x02329;" separators=""><mml:mrow><mml:mi mathvariant="bold">N</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="script">H</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">L</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>,</mml:mo><mml:msub><mml:mrow><mml:mi mathvariant="bold">&#x0039b;</mml:mi></mml:mrow><mml:mn>2</mml:mn></mml:msub></mml:mrow></mml:mfenced></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow/></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>+</mml:mo><mml:mfrac><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mn>2</mml:mn></mml:mfrac><mml:mrow><mml:mo maxsize="1.623em" minsize="1.623em" stretchy="true">(</mml:mo></mml:mrow><mml:mfenced close="|" open="|" separators=""><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="bold">M</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="bold">L</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="bold">E</mml:mi></mml:mfenced><mml:msubsup><mml:mrow><mml:msubsup><mml:mo stretchy="false">|</mml:mo><mml:mi>F</mml:mi><mml:mn>2</mml:mn></mml:msubsup><mml:mo>+</mml:mo><mml:mfenced close="|" open="|" separators=""><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="bold">N</mml:mi><mml:mo>-</mml:mo><mml:mi mathvariant="script">H</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">L</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mfenced><mml:mo stretchy="false">|</mml:mo></mml:mrow><mml:mi>F</mml:mi><mml:mn>2</mml:mn></mml:msubsup><mml:mrow><mml:mo maxsize="1.623em" minsize="1.623em" stretchy="true">)</mml:mo></mml:mrow><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11263_2016_985_Article_Equ13.gif" position="anchor"/></alternatives></disp-formula>where <inline-formula id="IEq166"><alternatives><tex-math id="M357">\documentclass[12pt]{minimal}
