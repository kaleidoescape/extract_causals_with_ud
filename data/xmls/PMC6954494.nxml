b-type="author"><name><surname>Ford</surname><given-names>Tamsin</given-names></name><xref ref-type="aff" rid="aff4">4</xref></contrib><contrib id="author-41344369" contrib-type="author"><name><surname>Prina</surname><given-names>A Matthew</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-65163332" contrib-type="author"><name><surname>Stuart</surname><given-names>Ruth</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-65163354" contrib-type="author"><name><surname>Livingstone</surname><given-names>Nuala</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-65163391" contrib-type="author"><name><surname>Kelly</surname><given-names>Grace</given-names></name><xref ref-type="aff" rid="aff5">5</xref></contrib><contrib id="author-65163410" contrib-type="author"><name><surname>Macdonald</surname><given-names>Geraldine</given-names></name><xref ref-type="aff" rid="aff6">6</xref></contrib><contrib id="author-65163417" contrib-type="author"><name><surname>Eisler</surname><given-names>Ivan</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-65163435" contrib-type="author"><name><surname>Gowers</surname><given-names>Simon</given-names></name><xref ref-type="aff" rid="aff7">7</xref></contrib><contrib id="author-44782687" contrib-type="author"><name><surname>Barrett</surname><given-names>Barbara M</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib id="author-43190512" contrib-type="author" corresp="yes"><contrib-id contrib-id-type="orcid" authenticated="false">http://orcid.org/0000-0001-7084-1495</contrib-id><name><surname>Byford</surname><given-names>Sarah</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib></contrib-group><aff id="aff1">
