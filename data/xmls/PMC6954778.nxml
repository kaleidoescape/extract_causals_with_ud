<name><surname>Maeda</surname><given-names>Tetsuya</given-names></name><xref ref-type="aff" rid="aff2">2</xref></contrib><contrib id="author-37655212" contrib-type="author" corresp="yes"><name><surname>Maemondo</surname><given-names>Makoto</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib></contrib-group><aff id="aff1">
