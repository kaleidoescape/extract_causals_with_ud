				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \pi ({\tilde{\mathbf {I}}}|\mathcal {G},\beta )=&#x00026;\,\prod _{i=1}^{m-1}\prod _{j=i+1}^{m}\left( \beta \exp [-\beta ({\tilde{I}}_j-{\tilde{I}}_i)]\right) ^{\varvec{1}\{\mathcal {G}_{ij}=1,\mathcal {P}_{ij}=1\}}\\&#x00026;\quad \times \left( \exp [-\beta ({\tilde{I}}_j-{\tilde{I}}_i)]\right) ^{\varvec{1}\{\mathcal {G}_{ij}=1,\mathcal {P}_{ij}\ne 1\}}\\ =&#x00026;\,\beta ^{m-1}\exp \left( -\beta \sum _{i=1}^{m-1}\sum _{j=i+1}^{m}\left[ ({\tilde{I}}_j-{\tilde{I}}_i)\varvec{1}\{\mathcal {G}_{ij}=1\}\right] \right) \\ =&#x00026;\,\beta ^{m-1}\exp \left( -\beta \sum _{i=1}^{m-1}\sum _{j=i+1}^{m}\mathcal {G}_{ij}({\tilde{I}}_j-{\tilde{I}}_i)\right) , \end{aligned}$$\end{document}</tex-math><mml:math id="M578" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi mathvariant="italic">&#x003c0;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mover accent="true"><mml:mi mathvariant="bold">I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="script">G</mml:mi><mml:mo>,</mml:mo><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mspace width="0.166667em"/><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:munderover><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mi>i</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:msup><mml:mfenced close=")" open="(" separators=""><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mo>exp</mml:mo><mml:mo stretchy="false">[</mml:mo><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>j</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo stretchy="false">]</mml:mo></mml:mfenced><mml:mrow><mml:mrow><mml:mn mathvariant="bold">1</mml:mn></mml:mrow><mml:mo stretchy="false">{</mml:mo><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo stretchy="false">}</mml:mo></mml:mrow></mml:msup></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow/></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mspace width="1em"/><mml:mo>&#x000d7;</mml:mo><mml:msup><mml:mfenced close=")" open="(" separators=""><mml:mo>exp</mml:mo><mml:mo stretchy="false">[</mml:mo><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>j</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo stretchy="false">]</mml:mo></mml:mfenced><mml:mrow><mml:mrow><mml:mn mathvariant="bold">1</mml:mn></mml:mrow><mml:mo stretchy="false">{</mml:mo><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>&#x02260;</mml:mo><mml:mn>1</mml:mn><mml:mo stretchy="false">}</mml:mo></mml:mrow></mml:msup></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mrow/><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mspace width="0.166667em"/><mml:msup><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msup><mml:mo>exp</mml:mo><mml:mfenced close=")" open="(" separators=""><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:munderover><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mi>i</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:mfenced close="]" open="[" separators=""><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>j</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:mn mathvariant="bold">1</mml:mn></mml:mrow><mml:mrow><mml:mo stretchy="false">{</mml:mo><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo stretchy="false">}</mml:mo></mml:mrow></mml:mfenced></mml:mfenced></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mrow/><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mspace width="0.166667em"/><mml:msup><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msup><mml:mo>exp</mml:mo><mml:mfenced close=")" open="(" separators=""><mml:mo>-</mml:mo><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:munderover><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mi>i</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>j</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfenced><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11222_2017_9770_Article_Equ15.gif" position="anchor"/></alternatives></disp-formula>which is the same as (<xref rid="Equ5" ref-type="">5</xref>), and confirms that the infection times are independent of the transmission tree, as the infection mechanism is a Poisson process (Britton and O&#x02019;Neill <xref ref-type="bibr" rid="CR7">2002</xref>), hence the dropping of <inline-formula id="IEq276"><alternatives><tex-math id="M579">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mathcal {P}$$\end{document}</tex-math><mml:math id="M580"><mml:mi mathvariant="script">P</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq276.gif"/></alternatives></inline-formula> in the function argument.</p><p id="Par40">Third, the contribution to likelihood by <inline-formula id="IEq277"><alternatives><tex-math id="M581">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mathcal {P}$$\end{document}</tex-math><mml:math id="M582"><mml:mi mathvariant="script">P</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq277.gif"/></alternatives></inline-formula> is straightforward, as <inline-formula id="IEq278"><alternatives><tex-math id="M583">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\pi (\mathcal {P}|\mathcal {G})$$\end{document}</tex-math><mml:math id="M584"><mml:mrow><mml:mi mathvariant="italic">&#x003c0;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="script">P</mml:mi><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="script">G</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq278.gif"/></alternatives></inline-formula> &#x0201c;is the uniform distribution on the set of all possible infection pathways&#x0201d; (Britton and O&#x02019;Neill <xref ref-type="bibr" rid="CR7">2002</xref>). We have to ensure nobody infects the initial infected (individual 1), and for each individual <italic>j</italic> of the remaining <inline-formula id="IEq279"><alternatives><tex-math id="M585">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$m-1$$\end{document}</tex-math><mml:math id="M586"><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq279.gif"/></alternatives></inline-formula> individuals, there is one and only one neighbour who is infected prior to individual <italic>j</italic>, ends up infecting individual <italic>j</italic>. Therefore,<disp-formula id="Equ16"><alternatives><tex-math id="M587">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \pi (\mathcal {P}|\mathcal {G}) =&#x00026;\mathbf {1}\{\text {nobody infects individual 1}\} \\&#x00026;\times \prod _{j=2}^m\frac{\mathbf {1}\{\text {only one previously infected neighbour infects individual { j}}\}}{\text {number of neighbours infected before individual} j} \\ =&#x00026;\prod _{i=2}^m\mathbf {1}\{\mathcal {P}_{i1}=0\}\times \prod _{j=2}^m \frac{\mathbf {1}\left\{ \sum \nolimits _{i=1}^{j-1}\mathcal {P}_{ij}=1\right\} \prod \nolimits _{i=j}^m\mathbf {1}\left\{ \mathcal {P}_{ij}=0\right\} \prod _{i=1}^m\mathbf {1}\left\{ \mathcal {P}_{ij}\le \mathcal {G}_{ij}\right\} }{\sum \nolimits _{i=1}^{j-1}\mathcal {G}_{ij}} \\ =&#x00026;\prod _{j=1}^{m-1}\prod _{i=j+1}^{m}(1-\mathcal {P}_{ij})\times \prod _{j=2}^{m}\frac{\mathbf {1}\left\{ \sum \nolimits _{i=1}^{j-1}\mathcal {P}_{ij}=1\right\} }{\sum \nolimits _{i=1}^{j-1}\mathcal {G}_{ij}}\times \prod _{j=2}^{m}\prod _{i=1}^{j-1}\mathbf {1}\left\{ \mathcal {P}_{ij}\le \mathcal {G}_{ij}\right\} , \end{aligned}$$\end{document}</tex-math><mml:math id="M588" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi mathvariant="italic">&#x003c0;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="script">P</mml:mi><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="script">G</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mn mathvariant="bold">1</mml:mn><mml:mo stretchy="false">{</mml:mo><mml:mtext>nobody infects individual 1</mml:mtext><mml:mo stretchy="false">}</mml:mo></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow/></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>&#x000d7;</mml:mo><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>2</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:mfrac><mml:mrow><mml:mn mathvariant="bold">1</mml:mn><mml:mo stretchy="false">{</mml:mo><mml:mtext>only one previously infected neighbour infects individual</mml:mtext><mml:mspace width="0.333333em"/><mml:mspace width="0.333333em"/><mml:mtext>j</mml:mtext><mml:mo stretchy="false">}</mml:mo></mml:mrow><mml:mrow><mml:mtext>number of neighbours infected before individual</mml:mtext><mml:mi>j</mml:mi></mml:mrow></mml:mfrac></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mrow/><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>2</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:mn mathvariant="bold">1</mml:mn><mml:mrow><mml:mo stretchy="false">{</mml:mo><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mn>1</mml:mn></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>0</mml:mn><mml:mo stretchy="false">}</mml:mo></mml:mrow><mml:mo>&#x000d7;</mml:mo><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>2</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:mfrac><mml:mrow><mml:mn mathvariant="bold">1</mml:mn><mml:mfenced close="}" open="{" separators=""><mml:msubsup><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>j</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msubsup><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mfenced><mml:msubsup><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mi>m</mml:mi></mml:msubsup><mml:mn mathvariant="bold">1</mml:mn><mml:mfenced close="}" open="{" separators=""><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>0</mml:mn></mml:mfenced><mml:msubsup><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:msubsup><mml:mn mathvariant="bold">1</mml:mn><mml:mfenced close="}" open="{" separators=""><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>&#x02264;</mml:mo><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub></mml:mfenced></mml:mrow><mml:mrow><mml:msubsup><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>j</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msubsup><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub></mml:mrow></mml:mfrac></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mrow/><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:munderover><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mi>j</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mn>1</mml:mn><mml:mo>-</mml:mo><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>&#x000d7;</mml:mo><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>2</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:mfrac><mml:mrow><mml:mn mathvariant="bold">1</mml:mn><mml:mfenced close="}" open="{" separators=""><mml:msubsup><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>j</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msubsup><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mfenced></mml:mrow><mml:mrow><mml:msubsup><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>j</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msubsup><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub></mml:mrow></mml:mfrac><mml:mo>&#x000d7;</mml:mo><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>2</mml:mn></mml:mrow><mml:mi>m</mml:mi></mml:munderover><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi>j</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:munderover><mml:mn mathvariant="bold">1</mml:mn><mml:mfenced close="}" open="{" separators=""><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>&#x02264;</mml:mo><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub></mml:mfenced><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11222_2017_9770_Article_Equ16.gif" position="anchor"/></alternatives></disp-formula>which is the same as (<xref rid="Equ4" ref-type="">4</xref>).</p></sec></app><app id="App2"><sec id="Sec11"><title>Appendix B: MCMC algorithm</title><p id="Par41">This appendix describes the MCMC algorithm for the inference outlined in Sect.&#x000a0;<xref rid="Sec6" ref-type="sec">3</xref>.</p><p id="Par42">
<bold>Sampling</bold>
<inline-formula id="IEq280"><alternatives><tex-math id="M589">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\beta $$\end{document}</tex-math><mml:math id="M590"><mml:mi mathvariant="italic">&#x003b2;</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq280.gif"/></alternatives></inline-formula> As we have assigned a (conditional) conjugate prior to <inline-formula id="IEq281"><alternatives><tex-math id="M591">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\beta $$\end{document}</tex-math><mml:math id="M592"><mml:mi mathvariant="italic">&#x003b2;</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq281.gif"/></alternatives></inline-formula>, its full conditional posterior is given by<disp-formula id="Equ17"><alternatives><tex-math id="M593">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \beta |\cdots \sim \text {Gamma}\left( a_{\beta }+m-1,~b_{\beta }+\sum _{1\le i&#x0003c;j\le m}\mathcal {G}_{ij}({\tilde{I}}_j-{\tilde{I}}_i)\right) , \end{aligned}$$\end{document}</tex-math><mml:math id="M594" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mrow><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mo stretchy="false">|</mml:mo><mml:mo>&#x022ef;</mml:mo><mml:mo>&#x0223c;</mml:mo><mml:mtext>Gamma</mml:mtext></mml:mrow><mml:mfenced close=")" open="(" separators=""><mml:msub><mml:mi>a</mml:mi><mml:mi mathvariant="italic">&#x003b2;</mml:mi></mml:msub><mml:mo>+</mml:mo><mml:mi>m</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mspace width="3.33333pt"/><mml:msub><mml:mi>b</mml:mi><mml:mi mathvariant="italic">&#x003b2;</mml:mi></mml:msub><mml:mo>+</mml:mo><mml:munder><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mn>1</mml:mn><mml:mo>&#x02264;</mml:mo><mml:mi>i</mml:mi><mml:mo>&#x0003c;</mml:mo><mml:mi>j</mml:mi><mml:mo>&#x02264;</mml:mo><mml:mi>m</mml:mi></mml:mrow></mml:munder><mml:msub><mml:mi mathvariant="script">G</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>j</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mover accent="true"><mml:mi>I</mml:mi><mml:mo stretchy="false">~</mml:mo></mml:mover><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfenced><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11222_2017_9770_Article_Equ17.gif" position="anchor"/></alternatives></disp-formula>where <inline-formula id="IEq282"><alternatives><tex-math id="M595">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\cdots $$\end{document}</tex-math><mml:math id="M596"><mml:mo>&#x022ef;</mml:mo></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq282.gif"/></alternatives></inline-formula> means all arguments in (<xref rid="Equ8" ref-type="">8</xref>) other than the quantity of interest. So, conditional upon the data and all other parameters, <inline-formula id="IEq283"><alternatives><tex-math id="M597">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\beta $$\end{document}</tex-math><mml:math id="M598"><mml:mi mathvariant="italic">&#x003b2;</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq283.gif"/></alternatives></inline-formula> can be sampled via a Gibbs step.</p><p id="Par43">
<bold>Sampling</bold>
<inline-formula id="IEq284"><alternatives><tex-math id="M599">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mu $$\end{document}</tex-math><mml:math id="M600"><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq284.gif"/></alternatives></inline-formula> As <inline-formula id="IEq285"><alternatives><tex-math id="M601">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mu $$\end{document}</tex-math><mml:math id="M602"><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq285.gif"/></alternatives></inline-formula> is only involved in one single term in the likelihood, namely <inline-formula id="IEq286"><alternatives><tex-math id="M603">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$L_1(\mathcal {G};\varvec{\sigma },\mu )$$\end{document}</tex-math><mml:math id="M604"><mml:mrow><mml:msub><mml:mi>L</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="script">G</mml:mi><mml:mo>&#x0037e;</mml:mo><mml:mrow><mml:mi mathvariant="bold-italic">&#x003c3;</mml:mi></mml:mrow><mml:mo>,</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mrow></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq286.gif"/></alternatives></inline-formula>, in the absence of a conjugate prior, we can update <inline-formula id="IEq287"><alternatives><tex-math id="M605">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mu $$\end{document}</tex-math><mml:math id="M606"><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq287.gif"/></alternatives></inline-formula> using a simple Metropolis step. Specifically, we propose <inline-formula id="IEq288"><alternatives><tex-math id="M607">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mu ^{*}$$\end{document}</tex-math><mml:math id="M608"><mml:msup><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq288.gif"/></alternatives></inline-formula> from a symmetrical proposal <inline-formula id="IEq289"><alternatives><tex-math id="M609">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$q(\cdot |\mu )$$\end{document}</tex-math><mml:math id="M610"><mml:mrow><mml:mi>q</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mo>&#x000b7;</mml:mo><mml:mo stretchy="false">|</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq289.gif"/></alternatives></inline-formula> and accept <inline-formula id="IEq290"><alternatives><tex-math id="M611">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\mu ^{*}$$\end{document}</tex-math><mml:math id="M612"><mml:msup><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup></mml:math><inline-graphic xlink:href="11222_2017_9770_Article_IEq290.gif"/></alternatives></inline-formula> with probability given by<disp-formula id="Equ18"><alternatives><tex-math id="M613">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
				\usepackage{wasysym} 
				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \alpha _{\mu }=1\wedge \frac{L_1(\mathcal {G};\varvec{\sigma },\mu ^{*})\times (\mu ^{*})^{a_\mu -1}\exp (-b_\mu \mu ^{*})\varvec{1}\{\mu ^{*}&#x0003e;0\}}{L_1(\mathcal {G};\varvec{\sigma },\mu )\times (\mu )^{a_\mu -1}\exp (-b_\mu \mu )\varvec{1}\{\mu &#x0003e;0\}}. \end{aligned}$$\end{document}</tex-math><mml:math id="M614" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi mathvariant="italic">&#x003b1;</mml:mi><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>&#x02227;</mml:mo><mml:mfrac><mml:mrow><mml:msub><mml:mi>L</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="script">G</mml:mi><mml:mo>&#x0037e;</mml:mo><mml:mrow><mml:mi mathvariant="bold-italic">&#x003c3;</mml:mi></mml:mrow><mml:mo>,</mml:mo><mml:msup><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>&#x000d7;</mml:mo><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msup><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:msub><mml:mi>a</mml:mi><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msup><mml:mo>exp</mml:mo><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mo>-</mml:mo><mml:msub><mml:mi>b</mml:mi><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:msub><mml:msup><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:mn mathvariant="bold">1</mml:mn></mml:mrow><mml:mrow><mml:mo stretchy="false">{</mml:mo><mml:msup><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msup><mml:mo>&#x0003e;</mml:mo><mml:mn>0</mml:mn><mml:mo stretchy="false">}</mml:mo></mml:mrow></mml:mrow><mml:mrow><mml:msub><mml:mi>L</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="script">G</mml:mi><mml:mo>&#x0037e;</mml:mo><mml:mrow><mml:mi mathvariant="bold-italic">&#x003c3;</mml:mi></mml:mrow><mml:mo>,</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>&#x000d7;</mml:mo><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:msub><mml:mi>a</mml:mi><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msup><mml:mo>exp</mml:mo><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mo>-</mml:mo><mml:msub><mml:mi>b</mml:mi><mml:mi mathvariant="italic">&#x003bc;</mml:mi></mml:msub><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:mn mathvariant="bold">1</mml:mn></mml:mrow><mml:mrow><mml:mo stretchy="false">{</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo>&#x0003e;</mml:mo><mml:mn>0</mml:mn><mml:mo stretchy="false">}</mml:mo></mml:mrow></mml:mrow></mml:mfrac><mml:mo>.</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11222_2017_9770_Article_Equ18.gif" position="anchor"/></alternatives></disp-formula>
<bold>Sampling</bold>
<inline-formula id="IEq291"><alternatives><tex-math id="M615">\documentclass[12pt]{minimal}
