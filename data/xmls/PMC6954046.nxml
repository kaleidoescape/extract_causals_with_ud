				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} R_{2\ell 4j} = {E_{\mathrm{T}}^{\mathrm{miss}}}/(E_{\mathrm{T}}^{\mathrm{miss}} + p_\mathrm{T}(\ell _1) + p_\mathrm{T}(\ell _2) + \sum _{i=1,\ldots ,N\le 4}p_\mathrm{T}(j_i)), \end{aligned}$$\end{document}</tex-math><mml:math id="M230" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi>R</mml:mi><mml:mrow><mml:mn>2</mml:mn><mml:mi>&#x02113;</mml:mi><mml:mn>4</mml:mn><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:msubsup><mml:mi>E</mml:mi><mml:mrow><mml:mi mathvariant="normal">T</mml:mi></mml:mrow><mml:mi mathvariant="normal">miss</mml:mi></mml:msubsup><mml:mo stretchy="false">/</mml:mo><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msubsup><mml:mi>E</mml:mi><mml:mrow><mml:mi mathvariant="normal">T</mml:mi></mml:mrow><mml:mi mathvariant="normal">miss</mml:mi></mml:msubsup><mml:mo>+</mml:mo><mml:msub><mml:mi>p</mml:mi><mml:mi mathvariant="normal">T</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>&#x02113;</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:msub><mml:mi>p</mml:mi><mml:mi mathvariant="normal">T</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>&#x02113;</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:munder><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mo>&#x02026;</mml:mo><mml:mo>,</mml:mo><mml:mi>N</mml:mi><mml:mo>&#x02264;</mml:mo><mml:mn>4</mml:mn></mml:mrow></mml:munder><mml:msub><mml:mi>p</mml:mi><mml:mi mathvariant="normal">T</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>j</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="10052_2017_5445_Article_Equ2.gif" position="anchor"/></alternatives></disp-formula>where <inline-formula id="IEq121"><alternatives><tex-math id="M231">\documentclass[12pt]{minimal}
