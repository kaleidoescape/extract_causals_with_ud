				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\frac{{k}_{nl}}{{k}_{l}}=1+4.4{{\rm{Re}}}_{B}^{0.4}{{\rm{\Pr }}}^{0.66}{(\frac{T}{{T}_{fr}})}^{10}{(\frac{{k}_{p}}{{k}_{l}})}^{0.03}{\varphi }^{0.66}.$$\end{document}</tex-math><mml:math id="M40" display="block"><mml:mfrac><mml:mrow><mml:msub><mml:mrow><mml:mi>k</mml:mi></mml:mrow><mml:mrow><mml:mi>n</mml:mi><mml:mi>l</mml:mi></mml:mrow></mml:msub></mml:mrow><mml:mrow><mml:msub><mml:mrow><mml:mi>k</mml:mi></mml:mrow><mml:mrow><mml:mi>l</mml:mi></mml:mrow></mml:msub></mml:mrow></mml:mfrac><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:mn>4.4</mml:mn><mml:msubsup><mml:mrow><mml:mi mathvariant="normal">Re</mml:mi></mml:mrow><mml:mrow><mml:mi>B</mml:mi></mml:mrow><mml:mrow><mml:mn>0.4</mml:mn></mml:mrow></mml:msubsup><mml:msup><mml:mrow><mml:mi mathvariant="normal">Pr</mml:mi></mml:mrow><mml:mrow><mml:mn>0.66</mml:mn></mml:mrow></mml:msup><mml:msup><mml:mrow><mml:mo stretchy="true">(</mml:mo><mml:mrow><mml:mfrac><mml:mi>T</mml:mi><mml:mrow><mml:msub><mml:mrow><mml:mi>T</mml:mi></mml:mrow><mml:mrow><mml:mi>f</mml:mi><mml:mi>r</mml:mi></mml:mrow></mml:msub></mml:mrow></mml:mfrac></mml:mrow><mml:mo stretchy="true">)</mml:mo></mml:mrow><mml:mrow><mml:mn>10</mml:mn></mml:mrow></mml:msup><mml:msup><mml:mrow><mml:mo stretchy="true">(</mml:mo><mml:mrow><mml:mfrac><mml:mrow><mml:msub><mml:mrow><mml:mi>k</mml:mi></mml:mrow><mml:mrow><mml:mi>p</mml:mi></mml:mrow></mml:msub></mml:mrow><mml:mrow><mml:msub><mml:mrow><mml:mi>k</mml:mi></mml:mrow><mml:mrow><mml:mi>l</mml:mi></mml:mrow></mml:msub></mml:mrow></mml:mfrac></mml:mrow><mml:mo stretchy="true">)</mml:mo></mml:mrow><mml:mrow><mml:mn>0.03</mml:mn></mml:mrow></mml:msup><mml:msup><mml:mrow><mml:mi>&#x003c6;</mml:mi></mml:mrow><mml:mrow><mml:mn>0.66</mml:mn></mml:mrow></mml:msup><mml:mo>.</mml:mo></mml:math><graphic xlink:href="41598_2019_57062_Article_Equ11.gif" position="anchor"/></alternatives></disp-formula>with<disp-formula id="Equ12"><label>12</label><alternatives><tex-math id="M41">\documentclass[12pt]{minimal}
